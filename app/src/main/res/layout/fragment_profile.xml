<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/home_bg"
    tools:context="agrospread.ProfileFragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        >


        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/SupplierPhoto"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="60dp"
            android:backgroundTint="@color/white"
            android:src="@drawable/supplier_profile_photo"
            app:shapeAppearanceOverlay="@style/circular" />

        <Button
            android:id="@+id/buttonSettings"
            android:clickable="true"
            android:focusable="true"
            style="@style/TransparentButton"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:drawableEnd="@drawable/nav_settings"
            tools:ignore="SpeakableTextPresentCheck" />

    </LinearLayout>

        <Button
            android:id="@+id/editPf"
            style="@style/TransparentButton"
            android:layout_width="40dp"
            android:layout_height="30dp"
            android:layout_marginStart="80dp"
            android:layout_marginTop="120dp"
            android:background="@drawable/shape"
            android:drawableStart="@drawable/edit"
            android:textAllCaps="false"
            android:textSize="5pt"
            tools:ignore="TextSizeCheck,TouchTargetSizeCheck,SpeakableTextPresentCheck" />



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_marginTop="250dp"
        android:layout_height="180dp">
        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            app:cardBackgroundColor="#FFFFFF"
            app:cardCornerRadius="20dp">


            <TextView
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginTop="10dp"
                android:text="My Orders"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:textSize="10pt"
                android:layout_marginStart="20dp"/>

            <Button
                android:id="@+id/payButton"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginStart="50dp"
                android:layout_marginTop="60dp"
                android:background="@drawable/pay"
                tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck" />

            <TextView
                android:layout_width="80dp"
                android:layout_height="60dp"
                android:layout_marginStart="35dp"
                android:layout_marginTop="100dp"
                android:text="Processing"
                android:textSize="6pt"
                android:textStyle="bold"
                tools:ignore="TextSizeCheck" />

            <Button
                android:id="@+id/processButton"
                android:layout_width="30dp"
                android:layout_height="25dp"
                android:layout_marginStart="150dp"
                android:layout_marginTop="63dp"
                android:background="@drawable/baseline_local_shipping_24"
                tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />
            <TextView
                android:layout_width="90dp"
                android:layout_height="60dp"
                android:layout_marginStart="140dp"
                android:layout_marginTop="100dp"
                android:text="To ship"
                android:textSize="7pt"
                android:textStyle="bold"/>

            <Button
                android:id="@+id/receiveButton"
                android:layout_width="48dp"
                android:layout_height="30dp"
                android:layout_marginStart="245dp"
                android:layout_marginTop="60dp"
                android:layout_marginEnd="30dp"
                android:background="@drawable/receive"
                tools:ignore="SpeakableTextPresentCheck" />
            <TextView
                android:layout_width="90dp"
                android:layout_height="60dp"
                android:layout_marginStart="230dp"
                android:layout_marginTop="100dp"
                android:text="To receive"
                android:textSize="7pt"
                android:textStyle="bold"/>

        </androidx.cardview.widget.CardView>
    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_marginTop="450dp"
        android:layout_height="180dp">
        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            app:cardBackgroundColor="#FFFFFF"
            app:cardCornerRadius="20dp"
            android:layout_height="150dp">


            <TextView
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginTop="10dp"
                android:text="Services"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:textSize="10pt"
                android:layout_marginStart="20dp"/>

            <Button
                android:id="@+id/addressButton"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginStart="50dp"
                android:layout_marginTop="60dp"
                android:background="@drawable/location"
                tools:ignore="TouchTargetSizeCheck" />
            <TextView
                android:layout_width="90dp"
                android:layout_height="60dp"
                android:layout_marginStart="30dp"
                android:layout_marginTop="100dp"
                android:text="Add adress"
                android:textSize="7pt"
                android:textStyle="bold"/>

            <Button
                android:id="@+id/sellingButton"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginStart="150dp"
                android:layout_marginTop="60dp"
                android:background="@drawable/sell"
                tools:ignore="SpeakableTextPresentCheck" />
            <TextView
                android:layout_width="90dp"
                android:layout_height="60dp"
                android:layout_marginStart="130dp"
                android:layout_marginTop="100dp"
                android:text="Start selling"
                android:textSize="7pt"
                android:textStyle="bold"/>

            <Button
                android:id="@+id/aboutButton"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginStart="245dp"
                android:layout_marginTop="60dp"
                android:layout_marginEnd="30dp"
                android:background="@drawable/about_us"
                tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />
            <TextView
                android:layout_width="90dp"
                android:layout_height="60dp"
                android:layout_marginStart="230dp"
                android:layout_marginTop="100dp"
                android:text="About us"
                android:textSize="7pt"
                android:textStyle="bold"/>

        </androidx.cardview.widget.CardView>
    </LinearLayout>

</RelativeLayout>
</ScrollView>